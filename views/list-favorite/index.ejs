<%- include('../includes/header.ejs'); %>

<div class="container">
    <% for(index in data) {%>
        <div class="card mb-3" style="max-width: 540px;">
            <div class="row g-0">
                <div class="col-md-4">
                <img src="<%- data[index].image %> " class="img-fluid rounded-start" alt="...">
                </div>
                <div class="col-md-8">
                <div class="card-body">
                    <h5 class="card-title"><%- data[index].title %>  </h5>
                    <p class="card-text"><%- data[index].caption %>  </p>
                    <i class="bi bi-star"></i>
                </div>
                </div>
            </div>
        </div>
    <% } %> 
</div>


<%- include('../includes/footer'); %>